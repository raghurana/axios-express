{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Axios Client Server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "tsx",
      "args": [
        "watch",
        "${workspaceFolder}/packages/axios-client/src/proxy-server.ts"
      ],
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    },
    {
      "name": "Launch Express Server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "tsx",
      "args": [
        "watch",
        "${workspaceFolder}/packages/express-server/src/server.ts"
      ],
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    }
  ],
  "compounds": [
    {
      "name": "Launch Both Servers",
      "configurations": ["Launch Axios Client Server", "Launch Express Server"]
    }
  ]
}
